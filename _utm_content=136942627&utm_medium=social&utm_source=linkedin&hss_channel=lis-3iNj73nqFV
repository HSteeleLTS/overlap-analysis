            if ( !$currentItem.hasClass('isOpened')){ // if opening, hide all the rest
                jQuery('.secondary-menu, .main-menu, .user-menu').removeClass('isOpened');
                $currentItem.addClass('isOpened');
            }
            else{
                $currentItem.removeClass('isOpened');
            }
            e.stopImmediatePropagation();
        });

    });
