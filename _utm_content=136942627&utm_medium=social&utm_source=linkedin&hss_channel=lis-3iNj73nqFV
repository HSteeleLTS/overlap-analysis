<script>

    function isValidEmail47405() {
        var pattern = /^([a-z\d!#$%&'*+\-\/=?^_`{|}~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]+(\.[a-z\d!#$%&'*+\-\/=?^_`{|}~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]+)*|"((([ \t]*\r\n)?[ \t]+)?([\x01-\x08\x0b\x0c\x0e-\x1f\x7f\x21\x23-\x5b\x5d-\x7e\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|\\[\x01-\x09\x0b\x0c\x0d-\x7f\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))*(([ \t]*\r\n)?[ \t]+)?")@(([a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|[a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF][a-z\d\-._~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]*[a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])\.)+([a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|[a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF][a-z\d\-._~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]*[a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])\.?$/i;
        return pattern.test(jQuery('#esf-47405 input[type=text]').val());
    };

    jQuery(document).ready(function () {

        jQuery('#esf-47405 button').on('click', function (e) {
            e.preventDefault();
            if (!isValidEmail47405()) {
                jQuery('.error-47405').fadeIn();
                jQuery('.success-47405').fadeOut();
                e.preventDefault();
            } else {
                jQuery('.error-47405').fadeOut();
                jQuery('.success-47405').fadeIn();
                var $form = jQuery(this).parents('form:first');
                jQuery.post(
                    'https://sloanreview.mit.edu/wp-admin/admin-ajax.php',
                    {
                        action: 'esf_subscribe',
                        email: jQuery("input[name='email']", $form).val(),
                        source: jQuery("input[name='source']", $form).val(),
                        list: jQuery("input[name='list']", $form).val(),
                        listID: jQuery("input[name='listID']", $form).val()
                    }
