<script>
var certonaVar;

function certonaRecommendations(response){
	if(Object.getOwnPropertyNames(response).length > 0){
		certonaVar = response;
		resultData = JSON.stringify(certonaVar);
		localStorage.setItem("certona",resultData);
	}
	
}
